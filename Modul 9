from termcolor import colored, cprint
for i in range(2):
    cprint(" "*26,"blue","on_blue")
for i in range(1):
    cprint(" "*26,"white","on_white")
    cprint(colored("          `VISA           ","blue","on_white",attrs=["bold"]))
    cprint(" "*26,"white","on_white")
for i in range(2):
    cprint(" "*26,"yellow","on_yellow")
